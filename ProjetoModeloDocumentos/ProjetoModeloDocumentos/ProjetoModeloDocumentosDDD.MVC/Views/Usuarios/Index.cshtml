@model IEnumerable<ProjetoModeloDocumentosDDD.MVC.ViewModels.UsuarioViewModel>

@{
    ViewBag.Title = "Index";
}
@if (Session["LogarUserIdComun"] != null || Session["LogarUserId"] != null)
{

    if (Session["LogarUserIdComun"] != null)
    {
        <td>
            @Html.ActionLink("Edit", "Edit", new {id = @Session["LogarUsuarioId"].ToString()}) |
            @Html.ActionLink("Details", "Details", new {id = @Session["LogarUsuarioId"].ToString()}) |
            @Html.ActionLink("Delete", "Delete", new {id = @Session["LogarUsuarioId"].ToString()})
        </td>
    }
    else
    {

        <h2>Usuario</h2>


        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SobreNome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cpf)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                


                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.SobreNome)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Cpf)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                   



                </tr>
            }

        </table>
    }
}
else
{
    <text>
        Login Obrigatorio!
    </text>
}
